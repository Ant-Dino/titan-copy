    "version": "0.2.0",
    "configurations": [
        

        {
            "type": "node",
            "request": "launch",
            "name": "Launch Program",
            "program": "${workspaceRoot}\\node_modules\\protractor\\bin\\protractor",
            "args": ["${workspaceRoot}/chrome.conf.js"],
            "cwd": "${workspaceFolder}",                       
            "outFiles": ["${workspaceRoot}/*/.js"],
            "sourceMaps": true,            
            "trace": true,
            "protocol": "inspector",
           
        }
    ]
}